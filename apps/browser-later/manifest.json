{
    "manifest_version": 2,
    "name": "Browser Later",
    "version": "0.1.0",
    "icons":{
        "16": "img/yep.png",
        "48": "img/yep.png",
        "128": "img/yep.png"

    },
    "content_scripts": [
    {
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "js/content.js"
        ]
    }],
    "background":
    {
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "contextMenus"
    ],
    "browser_action":
    {
        "default_popup": "popup.html",
        "default_title": "Browser Later"
    }
}